proc            /proc     proc   defaults                                                0  0
/dev/mmcblk0p1  /boot     vfat   defaults                                                0  2
/dev/mmcblk0p2  /         ext4   defaults,noatime,nodiratime                             0  1
tmpfs           /tmp      tmpfs  defaults,noatime,nodiratime,nosuid,mode=1777,size=100m  0  0
tmpfs           /var/log  tmpfs  defaults,noatime,nodiratime,nosuid,mode=0644,size=100m  0  0
